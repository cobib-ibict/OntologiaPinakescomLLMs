<?xml version="1.0"?>
<rdf:RDF xmlns="http://pinakes.prd.ibict.br/ontology/pinakes#"
     xml:base="http://pinakes.prd.ibict.br/ontology/pinakes"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes">
        <dc:rights rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://creativecommons.org/licenses/by/4.0/</dc:rights>
        <rdfs:comment xml:lang="pt-br">Esta Ontologia foi desenvolvida com base nos modelos LRM e PRESSoo, para a representação do domínio de serviços bibliográficos.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Modelo de dados que representa um conjunto de conceitos e os relacionamentos que abrangem os serviços bibliográficos integrados ao projeto Pinakes.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="pt-br">Ontologia Pinakes</rdfs:label>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Res">
        <rdfs:label xml:lang="pt-br">Recurso</rdfs:label>
        <rdfs:comment xml:lang="pt-br">A classe Res representa qualquer entidade ou conceito no domínio bibliográfico.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Classe base para todas as entidades da ontologia Pinakes, equivalente a LRM-E1: RES.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Obra">
        <rdfs:label xml:lang="pt-br">Obra</rdfs:label>
        <rdfs:comment xml:lang="pt-br">A classe Obra representa uma criação intelectual ou artística.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com LRM-E2: Obra. É superclasse de PublicacaoSeriada e PublicacaoRelacionada.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Res"/>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#PublicacaoSeriada">
        <rdfs:label xml:lang="pt-br">Publicação Seriada</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa uma publicação que é emitida em partes sucessivas, geralmente em intervalos regulares ou irregulares, e que se destina a continuar indefinidamente.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Classe para publicações seriais, alinhada com MP-C2 do modelo CCN e SerialWork do PRESSoo.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Obra"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#hasISSN"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#hasPublicationLanguage"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#PublicacaoRelacionada">
        <rdfs:label xml:lang="pt-br">Publicação Relacionada</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa uma obra que possui alguma relação com outra obra, como fusão, continuação, subdivisão, suplementação, etc.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C3 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Obra"/>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Expressao">
        <rdfs:label xml:lang="pt-br">Expressão</rdfs:label>
        <rdfs:comment xml:lang="pt-br">A classe Expressao representa a realização de uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com LRM-E3: Expressão.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Res"/>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Idioma">
        <rdfs:label xml:lang="pt-br">Idioma</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa o idioma de uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C4 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Expressao"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Manifestacao">
        <rdfs:label xml:lang="pt-br">Manifestação</rdfs:label>
        <rdfs:comment xml:lang="pt-br">A classe Manifestacao representa a corporificação de uma expressão.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com LRM-E4: Manifestação.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Res"/>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#MeioFisico">
        <rdfs:label xml:lang="pt-br">Meio Físico</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa a forma física ou o suporte material de uma manifestação.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C5 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Manifestacao"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Item">
        <rdfs:label xml:lang="pt-br">Item</rdfs:label>
        <rdfs:comment xml:lang="pt-br">A classe Item representa um exemplar de uma manifestação.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com LRM-E5: Item.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Res"/>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Colecao">
        <rdfs:label xml:lang="pt-br">Coleção</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa um conjunto de itens disponibilizados por uma biblioteca.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C6 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Item"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Agente">
        <rdfs:label xml:lang="pt-br">Agente</rdfs:label>
        <rdfs:comment xml:lang="pt-br">A classe Agente representa uma pessoa, família ou entidade corporativa responsável por uma atividade.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com LRM-E6: Agente.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Res"/>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Editora">
        <rdfs:label xml:lang="pt-br">Editora</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa a entidade responsável pela publicação de uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C7 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Agente"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#AgenteColetivo">
        <rdfs:label xml:lang="pt-br">Agente Coletivo</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa um grupo de agentes que atuam coletivamente.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com LRM-E8: Agente Coletivo.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Agente"/>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Instituicao">
        <rdfs:label xml:lang="pt-br">Instituição</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa uma organização ou entidade, como o Instituto Brasileiro de Informação em Ciência e Tecnologia (Ibict).</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C8 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#AgenteColetivo"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Biblioteca">
        <rdfs:label xml:lang="pt-br">Biblioteca</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa uma biblioteca, com suas características e serviços.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C9 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Instituicao"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Unidade">
        <rdfs:label xml:lang="pt-br">Unidade</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa uma unidade dentro de uma instituição.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C10 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Instituicao"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Nomen">
        <rdfs:label xml:lang="pt-br">Nomen</rdfs:label>
        <rdfs:comment xml:lang="pt-br">A classe Nomen representa um nome ou designação.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com LRM-E9: Nomen.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Res"/>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#ISSN">
        <rdfs:label xml:lang="pt-br">ISSN</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa o International Standard Serial Number (ISSN) de uma publicação seriada.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C11 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Nomen"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#descricao"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#TipoDeBiblioteca">
        <rdfs:label xml:lang="pt-br">Tipo de Biblioteca</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa a categorização de uma biblioteca, como "Biblioteca Setorial".</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C12 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Nomen"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#AssuntoControlado">
        <rdfs:label xml:lang="pt-br">Assunto Controlado</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa termos de assunto provenientes de vocabulários controlados.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C13 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Nomen"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#TermoLivre">
        <rdfs:label xml:lang="pt-br">Termo Livre</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa termos de assunto não controlados, atribuídos a uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C14 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Nomen"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#TabelaAreaDeConhecimento">
        <rdfs:label xml:lang="pt-br">Tabela de Área de Conhecimento</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa a fonte oficial de categorização para áreas do conhecimento.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C15 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Nomen"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Lugar">
        <rdfs:label xml:lang="pt-br">Lugar</rdfs:label>
        <rdfs:comment xml:lang="pt-br">A classe Lugar representa uma localização geográfica ou espacial.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com LRM-E10: Lugar. É superclasse de País, UnidadeFederativa, Regiao, Cidade e Endereco.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Res"/>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Pais">
        <rdfs:label xml:lang="pt-br">País</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa um país ou território geográfico.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C16 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Lugar"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#UnidadeFederativa">
        <rdfs:label xml:lang="pt-br">Unidade Federativa</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa uma unidade federativa ou estado.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C17 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Lugar"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Regiao">
        <rdfs:label xml:lang="pt-br">Região</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa uma região geográfica.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C18 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Lugar"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Localidade">
        <rdfs:label xml:lang="pt-br">Localidade</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa uma cidade ou localidade.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C19 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Lugar"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#nome"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Endereco">
        <rdfs:label xml:lang="pt-br">Endereço</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa o endereço físico de uma entidade.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Alinhada com MP-C20 do modelo CCN.</rdfs:isDefinedBy>
        <rdfs:subClassOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Lugar"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#logradouro"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Nota">
        <rdfs:label xml:lang="pt-br">Nota</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Representa informações adicionais sobre uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Classe para notas gerais, de complexidade e de suplemento.</rdfs:isDefinedBy>
