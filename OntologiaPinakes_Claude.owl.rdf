    <!-- ==============================================
         PROPRIEDADES DE OBJETO (OBJECT PROPERTIES)
         ============================================== -->

    <!-- Propriedades com suas inversas -->
    <owl:ObjectProperty rdf:about="#hasPublicationLanguage">
        <rdfs:domain rdf:resource="#PublicacaoSeriada"/>
        <rdfs:range rdf:resource="#Idioma"/>
        <rdfs:label xml:lang="pt-br">tem idioma de publicação</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Relaciona uma publicação seriada com seu idioma principal.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Vínculo entre uma obra e a língua em que foi expressa.</rdfs:isDefinedBy>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isPublicationLanguageOf">
        <owl:inverseOf rdf:resource="#hasPublicationLanguage"/>
        <rdfs:domain rdf:resource="#Idioma"/>
        <rdfs:range rdf:resource="#PublicacaoSeriada"/>
        <rdfs:label xml:lang="pt-br">é idioma de publicação de</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Relaciona um idioma com as publicações expressas nessa língua.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Inversa da propriedade que liga idioma à obra correspondente.</rdfs:isDefinedBy>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasISSN">
        <rdfs:domain rdf:resource="#PublicacaoSeriada"/>
        <rdfs:range rdf:resource="#ISSN"/>
        <rdfs:label xml:lang="pt-br">tem ISSN</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Relaciona uma publicação seriada com seu identificador ISSN.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Vínculo entre uma publicação e seu código internacional padronizado.</rdfs:isDefinedBy>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isISSNOf">
        <owl:inverseOf rdf:resource="#hasISSN"/>
        <rdfs:domain rdf:resource="#ISSN"/>
        <rdfs:range rdf:resource="#PublicacaoSeriada"/>
        <rdfs:label xml:lang="pt-br">é ISSN de</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Relaciona um código ISSN com sua publicação correspondente.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Inversa da propriedade que liga ISSN à publicação identificada.</rdfs:isDefinedBy>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasPublisher">
        <rdfs:domain rdf:resource="#PublicacaoImprenta"/>
        <rdfs:range rdf:resource="#Editora"/>
        <rdfs:label xml:lang="pt-br">tem editora</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Relaciona informações de imprenta com a editora responsável.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Vínculo entre dados de publicação e a organização editora.</rdfs:isDefinedBy>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isPublisherOf">
        <owl:inverseOf rdf:resource="#hasPublisher"/>
        <rdfs:domain rdf:resource="#Editora"/>
        <rdfs:range rdf:resource="#PublicacaoImprenta"/>
        <rdfs:label xml:lang="pt-br">é editora de</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Relaciona uma editora com as obras que publica.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Inversa da propriedade que conecta editora às suas publicações.</rdfs:isDefinedBy>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasCollection">
        <rdfs:domain rdf:resource="#Biblioteca"/>
        <rdfs:range rdf:resource="#Colecao"/>
        <rdfs:label xml:lang="pt-br">tem coleção</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Relaciona uma biblioteca com suas coleções de recursos.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Vínculo entre uma instituição e os acervos que mantém.</rdfs:isDefinedBy>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isCollectionOf">
