<?xml version="1.0"?>
<rdf:RDF xmlns="http://pinakes.prd.ibict.br/ontology/pinakes#"
     xml:base="http://pinakes.prd.ibict.br/ontology/pinakes"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes">
        <dc:rights rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://creativecommons.org/licenses/by/4.0/</dc:rights>
        <rdfs:comment xml:lang="pt-br">Esta Ontologia foi desenvolvida com base nos modelos LRM e PRESSoo, para a representação do domínio de serviços bibliográficos.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Modelo de dados que representa um conjunto de conceitos e os relacionamentos que abrangem os serviços bibliográficos integrados ao projeto Pinakes.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="pt-br">Ontologia Pinakes</rdfs:label>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>

    <!-- ==============================================
         CLASSES HIERÁRQUICAS BASEADAS NO ALINHAMENTO LRM
         ============================================== -->

    <!-- Classe raiz Res do LRM -->
    <owl:Class rdf:about="#Res">
        <rdfs:label xml:lang="pt-br">Recurso</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Classe raiz que representa qualquer recurso do modelo LRM. Superclasse de todas as entidades bibliográficas.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Entidade fundamental do IFLA LRM que engloba tudo que pode ser identificado.</rdfs:isDefinedBy>
    </owl:Class>

    <!-- Superclasses do LRM -->
    <owl:Class rdf:about="#Obra">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Obra</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Criação intelectual ou artística distinta, expressa em linguagem, música, forma notacional ou movimento.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Entidade do IFLA LRM que representa o conteúdo conceitual distinto de uma criação.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Expressao">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Expressão</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Realização intelectual ou artística de uma obra na forma de notação alfa-numérica, musical ou coreográfica, som, imagem, objeto ou movimento.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Entidade do IFLA LRM que representa a forma específica na qual uma obra é realizada.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Manifestacao">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Manifestação</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Conjunto de características físicas compartilhadas por exemplares que corporificam a mesma expressão de uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Entidade do IFLA LRM que representa a forma física na qual uma expressão é materializada.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Item">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Item</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Exemplar individual de uma manifestação.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Entidade do IFLA LRM que representa um objeto físico ou digital específico.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Agente">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Agente</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Entidade capaz de ação deliberada, responsável pela custódia, propriedade ou criação de recursos.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Entidade do IFLA LRM que representa pessoas ou organizações que agem sobre recursos.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#AgenteColetivo">
        <rdfs:subClassOf rdf:resource="#Agente"/>
        <rdfs:label xml:lang="pt-br">Agente Coletivo</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Organização ou grupo de pessoas ou organizações identificado por um nome particular.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Subtipo de agente que representa organizações, corporações, famílias ou grupos.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Nomen">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Nome</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Denominação, designação, título ou qualquer combinação de palavras ou caracteres que serve para identificar uma entidade.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Entidade do IFLA LRM para representar nomes, títulos e identificadores.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Lugar">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Lugar</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Extensão específica de espaço tridimensional.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Entidade do IFLA LRM que representa localizações geográficas ou conceituais.</rdfs:isDefinedBy>
    </owl:Class>

    <!-- Classes específicas do CCN baseadas na hierarquia -->
    <owl:Class rdf:about="#PublicacaoSeriada">
        <rdfs:subClassOf rdf:resource="#Obra"/>
        <rdfs:label xml:lang="pt-br">Publicação Seriada</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Publicação em qualquer suporte físico, editada em unidades sucessivas, numeradas ou datadas, com intenção de continuidade indefinida.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Tipo específico de obra caracterizada pela continuidade e periodicidade de publicação.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#PublicacaoRelacionada">
        <rdfs:subClassOf rdf:resource="#Obra"/>
        <rdfs:label xml:lang="pt-br">Publicação Relacionada</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Obra que possui relação bibliográfica com outra obra através de continuação, fusão, divisão ou outros tipos de ligação.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Obra que mantém vínculos específicos com outras obras do catálogo.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Idioma">
        <rdfs:subClassOf rdf:resource="#Expressao"/>
        <rdfs:label xml:lang="pt-br">Idioma</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Sistema de comunicação verbal ou escrito usado para expressar uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Forma linguística na qual uma obra é expressa.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#MeioFisico">
        <rdfs:subClassOf rdf:resource="#Manifestacao"/>
        <rdfs:label xml:lang="pt-br">Meio Físico</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Suporte material ou formato em que uma manifestação é disponibilizada.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Características físicas ou digitais que definem como uma manifestação se apresenta.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Colecao">
        <rdfs:subClassOf rdf:resource="#Item"/>
        <rdfs:label xml:lang="pt-br">Coleção</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Conjunto de itens bibliográficos organizados e mantidos por uma biblioteca.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Agrupamento de itens específicos que constituem o acervo de uma instituição.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Editora">
        <rdfs:subClassOf rdf:resource="#Agente"/>
        <rdfs:label xml:lang="pt-br">Editora</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Agente responsável pela edição e publicação de obras.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Entidade que assume a responsabilidade editorial e comercial de uma publicação.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Biblioteca">
        <rdfs:subClassOf rdf:resource="#AgenteColetivo"/>
        <rdfs:label xml:lang="pt-br">Biblioteca</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Organização responsável por coletar, organizar, preservar e disponibilizar recursos informacionais.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Instituição dedicada aos serviços bibliográficos e informacionais.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Unidade">
        <rdfs:subClassOf rdf:resource="#AgenteColetivo"/>
        <rdfs:label xml:lang="pt-br">Unidade</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Divisão organizacional ou departamento dentro de uma instituição.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Subdivisão administrativa de uma organização maior.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Instituicao">
        <rdfs:subClassOf rdf:resource="#AgenteColetivo"/>
        <rdfs:label xml:lang="pt-br">Instituição</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Organização estabelecida para fins específicos, como educação, pesquisa ou serviços públicos.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Entidade coletiva com estrutura organizacional formal e objetivos definidos.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#ISSN">
        <rdfs:subClassOf rdf:resource="#Nomen"/>
        <rdfs:label xml:lang="pt-br">ISSN</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Número Internacional Normalizado para Publicações Seriadas, identificador único de publicações periódicas.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Código numérico padronizado internacionalmente para identificação unívoca de publicações seriadas.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#TituloComplementar">
        <rdfs:subClassOf rdf:resource="#Nomen"/>
        <rdfs:label xml:lang="pt-br">Título Complementar</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Variação ou forma alternativa do título principal de uma obra, incluindo títulos abreviados, anteriores ou paralelos.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Denominação adicional que complementa ou substitui o título principal para fins de identificação.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#AssuntoControlado">
        <rdfs:subClassOf rdf:resource="#Nomen"/>
        <rdfs:label xml:lang="pt-br">Assunto Controlado</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Termo de indexação proveniente de vocabulário controlado usado para representar o conteúdo temático de uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Descritor padronizado extraído de tesauro ou vocabulário controlado para indexação temática.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#AreaDeConhecimento">
        <rdfs:subClassOf rdf:resource="#Nomen"/>
        <rdfs:label xml:lang="pt-br">Área de Conhecimento</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Campo ou domínio científico ao qual uma obra pertence, segundo classificação oficial.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Categoria temática que agrupa conhecimentos afins segundo taxonomia acadêmica estabelecida.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#TabelaAreaDeConhecimento">
        <rdfs:subClassOf rdf:resource="#Nomen"/>
        <rdfs:label xml:lang="pt-br">Tabela de Área de Conhecimento</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Sistema de classificação oficial usado para organizar e categorizar áreas do conhecimento.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Esquema classificatório autorizado para estruturação de domínios científicos e acadêmicos.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Pais">
        <rdfs:subClassOf rdf:resource="#Lugar"/>
        <rdfs:label xml:lang="pt-br">País</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Território geográfico com governo soberano e limites definidos.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Unidade política e geográfica de nível nacional.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#UnidadeFederativa">
        <rdfs:subClassOf rdf:resource="#Lugar"/>
        <rdfs:label xml:lang="pt-br">Unidade Federativa</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Divisão política administrativa de um país federativo, como estado ou província.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Subdivisão territorial com autonomia política dentro de um sistema federativo.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Regiao">
        <rdfs:subClassOf rdf:resource="#Lugar"/>
        <rdfs:label xml:lang="pt-br">Região</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Divisão geográfica que agrupa unidades federativas por características comuns.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Agrupamento territorial baseado em critérios geográficos, econômicos ou culturais.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Localidade">
        <rdfs:subClassOf rdf:resource="#Lugar"/>
        <rdfs:label xml:lang="pt-br">Localidade</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Área urbana ou rural específica, como cidade, vila ou distrito.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Local específico onde se situam instituições, bibliotecas ou onde obras são publicadas.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Endereco">
        <rdfs:subClassOf rdf:resource="#Lugar"/>
        <rdfs:label xml:lang="pt-br">Endereço</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Localização específica de uma instituição, incluindo logradouro, número e complementos.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Dados precisos de localização física de uma entidade ou organização.</rdfs:isDefinedBy>
    </owl:Class>

    <!-- Classes auxiliares do domínio -->
    <owl:Class rdf:about="#Nota">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Nota</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Informação explicativa ou complementar sobre uma obra ou recurso bibliográfico.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Texto informativo adicional que esclarece aspectos específicos de um recurso.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Referenciador">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Referenciador</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Base de dados ou serviço que indexa e referencia publicações para fins de descoberta e acesso.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Sistema ou serviço responsável pela indexação e disseminação de informações bibliográficas.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#PublicacaoImprenta">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Publicação Imprenta</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Informações sobre local, editora e data de publicação de uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Dados que identificam as circunstâncias editoriais de uma publicação.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Contato">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Contato</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Informações para comunicação com uma biblioteca ou instituição.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Dados que permitem estabelecer comunicação com uma organização.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#Homepage">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Página Web</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Endereço eletrônico principal de uma obra ou instituição na internet.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Recurso digital que representa a presença oficial de uma entidade na web.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#NaturezaAdministrativa">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Natureza Administrativa</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Categoria jurídica ou administrativa que caracteriza uma instituição.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Classificação legal que define o tipo organizacional de uma entidade.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#TipoDeBiblioteca">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Tipo de Biblioteca</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Categoria funcional que classifica uma biblioteca segundo sua função institucional.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Tipologia que define o papel e escopo de atuação de uma biblioteca.</rdfs:isDefinedBy>
    </owl:Class>

    <owl:Class rdf:about="#TermoLivre">
        <rdfs:subClassOf rdf:resource="#Res"/>
        <rdfs:label xml:lang="pt-br">Termo Livre</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Palavra-chave não controlada usada para descrever o conteúdo de uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Descritor de linguagem natural empregado na indexação livre de recursos.</rdfs:isDefinedBy>
    </owl:Class>

    <!-- ==============================================
         PROPRIEDADES DE DADOS (DATA PROPERTIES)
         ============================================== -->

    <!-- Propriedade raiz para designações numéricas -->
    <owl:DatatypeProperty rdf:about="#designacaoNumerica">
        <rdfs:domain rdf:resource="#Obra"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="pt-br">designação numérica</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Numeração ou designação sequencial de uma obra, como volume, número, edição.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Identificação numérica que especifica a posição de uma unidade numa sequência.</rdfs:isDefinedBy>
    </owl:DatatypeProperty>

    <!-- Subpropriedades de designação numérica -->
    <owl:DatatypeProperty rdf:about="#volume">
        <rdfs:subPropertyOf rdf:resource="#designacaoNumerica"/>
        <rdfs:domain rdf:resource="#PublicacaoSeriada"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="pt-br">volume</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Número do volume de uma publicação seriada.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Designação numérica que identifica um conjunto de fascículos de uma publicação periódica.</rdfs:isDefinedBy>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#numero">
        <rdfs:subPropertyOf rdf:resource="#designacaoNumerica"/>
        <rdfs:domain rdf:resource="#PublicacaoSeriada"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="pt-br">número</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Número do fascículo ou edição de uma publicação seriada.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Designação numérica sequencial de um fascículo dentro de um volume.</rdfs:isDefinedBy>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#edicao">
        <rdfs:subPropertyOf rdf:resource="#designacaoNumerica"/>
        <rdfs:domain rdf:resource="#Obra"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="pt-br">edição</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Número ou designação da edição de uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Identificação da versão editorial específica de uma obra.</rdfs:isDefinedBy>
    </owl:DatatypeProperty>

    <!-- Outras propriedades de dados -->
    <owl:DatatypeProperty rdf:about="#tituloProprio">
        <rdfs:domain rdf:resource="#Obra"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="pt-br">título próprio</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Título principal pelo qual uma obra é formalmente identificada.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Denominação principal e oficial de uma obra conforme registrada.</rdfs:isDefinedBy>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#conteudo">
        <rdfs:domain rdf:resource="#Obra"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="pt-br">conteúdo</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Tipologia documental que caracteriza o tipo de conteúdo de uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Categoria que classifica uma obra segundo sua natureza intelectual.</rdfs:isDefinedBy>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#alfabetacao">
        <rdfs:domain rdf:resource="#Obra"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="pt-br">alfabetação</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Posição de início do título para fins de ordenação alfabética.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Indicador numérico da posição inicial para alfabetação do título.</rdfs:isDefinedBy>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#nivelDeCatalogacao">
        <rdfs:domain rdf:resource="#PublicacaoSeriada"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="pt-br">nível de catalogação</rdfs:label>
        <rdfs:comment xml:lang="pt-br">Grau de detalhamento aplicado na catalogação de uma obra.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="pt-br">Indicação do nível de completude e precisão dos dados catalográficos.</rdfs:isDefinedBy>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#sigla">
        <rdfs:domain rdf:resource="#Res"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="pt-br">sigla</rdfs:label>
